; Copy 'LethalXcess' entry directory
IF EXISTS "USERPACKAGEDIR:L/LethalXcess.info"
  Copy >NIL: "USERPACKAGEDIR:L/LethalXcess.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "L/LethalXcess"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:L/LethalXcess"
  Copy >NIL: "USERPACKAGEDIR:L/LethalXcess" "$entrydir" ALL
ENDIF

