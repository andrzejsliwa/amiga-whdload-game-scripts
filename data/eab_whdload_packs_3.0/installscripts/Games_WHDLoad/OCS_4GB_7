; Copy 'InsanityFight' entry directory
IF EXISTS "USERPACKAGEDIR:I/InsanityFight.info"
  Copy >NIL: "USERPACKAGEDIR:I/InsanityFight.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "I/InsanityFight"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:I/InsanityFight"
  Copy >NIL: "USERPACKAGEDIR:I/InsanityFight" "$entrydir" ALL
ENDIF

