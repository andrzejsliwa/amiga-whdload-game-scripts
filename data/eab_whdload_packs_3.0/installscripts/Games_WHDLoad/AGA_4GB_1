; '0-9' index directory
echo "Installing '0-9'..."
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "0-9"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:0-9.info"
  Copy >NIL: "USERPACKAGEDIR:0-9.info" "$INSTALLDIR"
ENDIF

; Copy '1000ccTurbo' entry directory
IF EXISTS "USERPACKAGEDIR:0-9/1000ccTurbo.info"
  Copy >NIL: "USERPACKAGEDIR:0-9/1000ccTurbo.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "0-9/1000ccTurbo"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:0-9/1000ccTurbo"
  Copy >NIL: "USERPACKAGEDIR:0-9/1000ccTurbo" "$entrydir" ALL
ENDIF

