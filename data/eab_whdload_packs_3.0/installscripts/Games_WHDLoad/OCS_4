; Copy 'Clockwiser' entry directory
IF EXISTS "USERPACKAGEDIR:C/Clockwiser.info"
  Copy >NIL: "USERPACKAGEDIR:C/Clockwiser.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "C/Clockwiser"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:C/Clockwiser"
  Copy >NIL: "USERPACKAGEDIR:C/Clockwiser" "$entrydir" ALL
ENDIF

