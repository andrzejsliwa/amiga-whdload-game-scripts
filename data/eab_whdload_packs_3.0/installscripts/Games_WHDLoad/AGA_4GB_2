; Copy 'BanditKings' entry directory
IF EXISTS "USERPACKAGEDIR:B/BanditKings.info"
  Copy >NIL: "USERPACKAGEDIR:B/BanditKings.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "B/BanditKings"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:B/BanditKings"
  Copy >NIL: "USERPACKAGEDIR:B/BanditKings" "$entrydir" ALL
ENDIF

