  Copy >NIL: "USERPACKAGEDIR:F/Freedom.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "F/Freedom"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:F/Freedom"
  Copy >NIL: "USERPACKAGEDIR:F/Freedom" "$entrydir" ALL
ENDIF

