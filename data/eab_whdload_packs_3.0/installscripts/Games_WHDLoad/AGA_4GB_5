; Copy 'F15StrikeEagle2' entry directory
IF EXISTS "USERPACKAGEDIR:F/F15StrikeEagle2.info"
  Copy >NIL: "USERPACKAGEDIR:F/F15StrikeEagle2.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "F/F15StrikeEagle2"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:F/F15StrikeEagle2"
  Copy >NIL: "USERPACKAGEDIR:F/F15StrikeEagle2" "$entrydir" ALL
ENDIF

