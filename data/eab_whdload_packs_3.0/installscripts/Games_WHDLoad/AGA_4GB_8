; Copy 'Mach3' entry directory
IF EXISTS "USERPACKAGEDIR:M/Mach3.info"
  Copy >NIL: "USERPACKAGEDIR:M/Mach3.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "M/Mach3"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:M/Mach3"
  Copy >NIL: "USERPACKAGEDIR:M/Mach3" "$entrydir" ALL
ENDIF

