  Copy >NIL: "USERPACKAGEDIR:I/Interphase.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "I/Interphase"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:I/Interphase"
  Copy >NIL: "USERPACKAGEDIR:I/Interphase" "$entrydir" ALL
ENDIF

