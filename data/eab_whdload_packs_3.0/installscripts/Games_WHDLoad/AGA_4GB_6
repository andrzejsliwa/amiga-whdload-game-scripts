; Copy 'GoldOfTheAztecs' entry directory
IF EXISTS "USERPACKAGEDIR:G/GoldOfTheAztecs.info"
  Copy >NIL: "USERPACKAGEDIR:G/GoldOfTheAztecs.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "G/GoldOfTheAztecs"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:G/GoldOfTheAztecs"
  Copy >NIL: "USERPACKAGEDIR:G/GoldOfTheAztecs" "$entrydir" ALL
ENDIF

