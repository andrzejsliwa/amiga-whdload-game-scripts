  Copy >NIL: "USERPACKAGEDIR:F/FutureShock.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "F/FutureShock"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:F/FutureShock"
  Copy >NIL: "USERPACKAGEDIR:F/FutureShock" "$entrydir" ALL
ENDIF

