; 'G' index directory
echo "Installing 'G'..."
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "G"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:G.info"
  Copy >NIL: "USERPACKAGEDIR:G.info" "$INSTALLDIR"
ENDIF

