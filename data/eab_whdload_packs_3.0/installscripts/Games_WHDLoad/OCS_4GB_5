  Copy >NIL: "USERPACKAGEDIR:F/Flashback.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "F/Flashback"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:F/Flashback"
  Copy >NIL: "USERPACKAGEDIR:F/Flashback" "$entrydir" ALL
ENDIF

