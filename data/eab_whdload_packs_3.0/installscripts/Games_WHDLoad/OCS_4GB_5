; Copy 'Enforcer' entry directory
IF EXISTS "USERPACKAGEDIR:E/Enforcer.info"
  Copy >NIL: "USERPACKAGEDIR:E/Enforcer.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "E/Enforcer"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:E/Enforcer"
  Copy >NIL: "USERPACKAGEDIR:E/Enforcer" "$entrydir" ALL
ENDIF

