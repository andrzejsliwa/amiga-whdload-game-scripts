  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:W/WinterSupersports92"
  Copy >NIL: "USERPACKAGEDIR:W/WinterSupersports92" "$entrydir" ALL
ENDIF

