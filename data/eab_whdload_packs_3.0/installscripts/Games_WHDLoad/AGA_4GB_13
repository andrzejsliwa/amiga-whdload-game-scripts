; Copy 'TreasuresOfSavageFrontier' entry directory
IF EXISTS "USERPACKAGEDIR:T/TreasuresOfSavageFrontier.info"
  Copy >NIL: "USERPACKAGEDIR:T/TreasuresOfSavageFrontier.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "T/TreasuresOfSavageFrontier"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:T/TreasuresOfSavageFrontier"
  Copy >NIL: "USERPACKAGEDIR:T/TreasuresOfSavageFrontier" "$entrydir" ALL
ENDIF

