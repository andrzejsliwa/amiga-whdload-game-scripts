  Copy >NIL: "USERPACKAGEDIR:S/SkeetShoot.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "S/SkeetShoot"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:S/SkeetShoot"
  Copy >NIL: "USERPACKAGEDIR:S/SkeetShoot" "$entrydir" ALL
ENDIF

