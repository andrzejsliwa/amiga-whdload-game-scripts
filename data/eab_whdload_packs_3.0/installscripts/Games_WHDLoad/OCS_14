; Copy 'RollOrDie' entry directory
IF EXISTS "USERPACKAGEDIR:R/RollOrDie.info"
  Copy >NIL: "USERPACKAGEDIR:R/RollOrDie.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "R/RollOrDie"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:R/RollOrDie"
  Copy >NIL: "USERPACKAGEDIR:R/RollOrDie" "$entrydir" ALL
ENDIF

