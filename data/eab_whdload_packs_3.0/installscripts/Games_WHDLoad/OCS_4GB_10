  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:R/RockChallenge"
  Copy >NIL: "USERPACKAGEDIR:R/RockChallenge" "$entrydir" ALL
ENDIF

