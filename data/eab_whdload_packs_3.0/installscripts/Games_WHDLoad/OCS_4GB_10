  Copy >NIL: "USERPACKAGEDIR:R/Realms.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "R/Realms"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:R/Realms"
  Copy >NIL: "USERPACKAGEDIR:R/Realms" "$entrydir" ALL
ENDIF

