; Copy 'Pool' entry directory
IF EXISTS "USERPACKAGEDIR:P/Pool.info"
  Copy >NIL: "USERPACKAGEDIR:P/Pool.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "P/Pool"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:P/Pool"
  Copy >NIL: "USERPACKAGEDIR:P/Pool" "$entrydir" ALL
ENDIF

