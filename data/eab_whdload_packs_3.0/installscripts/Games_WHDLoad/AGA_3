; Copy 'Bombuzal' entry directory
IF EXISTS "USERPACKAGEDIR:B/Bombuzal.info"
  Copy >NIL: "USERPACKAGEDIR:B/Bombuzal.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "B/Bombuzal"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:B/Bombuzal"
  Copy >NIL: "USERPACKAGEDIR:B/Bombuzal" "$entrydir" ALL
ENDIF

