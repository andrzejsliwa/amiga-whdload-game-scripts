; Copy 'Jet&SceneryDisk13' entry directory
IF EXISTS "USERPACKAGEDIR:J/Jet&SceneryDisk13.info"
  Copy >NIL: "USERPACKAGEDIR:J/Jet&SceneryDisk13.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "J/Jet&SceneryDisk13"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:J/Jet&SceneryDisk13"
  Copy >NIL: "USERPACKAGEDIR:J/Jet&SceneryDisk13" "$entrydir" ALL
ENDIF

