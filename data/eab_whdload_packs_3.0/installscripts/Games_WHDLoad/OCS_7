; Copy 'FootballManager2' entry directory
IF EXISTS "USERPACKAGEDIR:F/FootballManager2.info"
  Copy >NIL: "USERPACKAGEDIR:F/FootballManager2.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "F/FootballManager2"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:F/FootballManager2"
  Copy >NIL: "USERPACKAGEDIR:F/FootballManager2" "$entrydir" ALL
ENDIF

