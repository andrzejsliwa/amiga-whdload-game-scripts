; Copy 'ZakMcKrackenDe' entry directory
IF EXISTS "USERPACKAGEDIR:Z/ZakMcKrackenDe.info"
  Copy >NIL: "USERPACKAGEDIR:Z/ZakMcKrackenDe.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "Z/ZakMcKrackenDe"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:Z/ZakMcKrackenDe"
  Copy >NIL: "USERPACKAGEDIR:Z/ZakMcKrackenDe" "$entrydir" ALL
ENDIF

