; Copy 'Carver' entry directory
IF EXISTS "USERPACKAGEDIR:C/Carver.info"
  Copy >NIL: "USERPACKAGEDIR:C/Carver.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "C/Carver"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:C/Carver"
  Copy >NIL: "USERPACKAGEDIR:C/Carver" "$entrydir" ALL
ENDIF

