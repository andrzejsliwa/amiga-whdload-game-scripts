; Copy 'PsychoSanta' entry directory
IF EXISTS "USERPACKAGEDIR:P/PsychoSanta.info"
  Copy >NIL: "USERPACKAGEDIR:P/PsychoSanta.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "P/PsychoSanta"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:P/PsychoSanta"
  Copy >NIL: "USERPACKAGEDIR:P/PsychoSanta" "$entrydir" ALL
ENDIF

