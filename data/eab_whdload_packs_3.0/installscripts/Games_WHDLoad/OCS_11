; Copy 'MeanStreets' entry directory
IF EXISTS "USERPACKAGEDIR:M/MeanStreets.info"
  Copy >NIL: "USERPACKAGEDIR:M/MeanStreets.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "M/MeanStreets"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:M/MeanStreets"
  Copy >NIL: "USERPACKAGEDIR:M/MeanStreets" "$entrydir" ALL
ENDIF

