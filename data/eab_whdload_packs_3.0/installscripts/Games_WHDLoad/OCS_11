  Copy >NIL: "USERPACKAGEDIR:N/NightHunter.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "N/NightHunter"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:N/NightHunter"
  Copy >NIL: "USERPACKAGEDIR:N/NightHunter" "$entrydir" ALL
ENDIF

