; Copy 'TimeRace' entry directory
IF EXISTS "USERPACKAGEDIR:T/TimeRace.info"
  Copy >NIL: "USERPACKAGEDIR:T/TimeRace.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "T/TimeRace"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:T/TimeRace"
  Copy >NIL: "USERPACKAGEDIR:T/TimeRace" "$entrydir" ALL
ENDIF

