; Copy 'Italia1990' entry directory
IF EXISTS "USERPACKAGEDIR:I/Italia1990.info"
  Copy >NIL: "USERPACKAGEDIR:I/Italia1990.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "I/Italia1990"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:I/Italia1990"
  Copy >NIL: "USERPACKAGEDIR:I/Italia1990" "$entrydir" ALL
ENDIF

