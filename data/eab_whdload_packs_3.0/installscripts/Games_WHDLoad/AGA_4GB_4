  Copy >NIL: "USERPACKAGEDIR:D/DungeonMaster.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "D/DungeonMaster"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:D/DungeonMaster"
  Copy >NIL: "USERPACKAGEDIR:D/DungeonMaster" "$entrydir" ALL
ENDIF

