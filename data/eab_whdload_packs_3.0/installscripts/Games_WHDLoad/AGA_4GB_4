ENDIF
IF EXISTS "USERPACKAGEDIR:E/Erik"
  Copy >NIL: "USERPACKAGEDIR:E/Erik" "$entrydir" ALL
ENDIF

; Copy 'EscapeFromColditz' entry directory
IF EXISTS "USERPACKAGEDIR:E/EscapeFromColditz.info"
  Copy >NIL: "USERPACKAGEDIR:E/EscapeFromColditz.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "E/EscapeFromColditz"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:E/EscapeFromColditz"
  Copy >NIL: "USERPACKAGEDIR:E/EscapeFromColditz" "$entrydir" ALL
ENDIF

