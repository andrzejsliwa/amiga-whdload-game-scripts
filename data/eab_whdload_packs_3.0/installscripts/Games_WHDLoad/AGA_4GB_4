; Copy 'DaysOfThunder' entry directory
IF EXISTS "USERPACKAGEDIR:D/DaysOfThunder.info"
  Copy >NIL: "USERPACKAGEDIR:D/DaysOfThunder.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "D/DaysOfThunder"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:D/DaysOfThunder"
  Copy >NIL: "USERPACKAGEDIR:D/DaysOfThunder" "$entrydir" ALL
ENDIF

