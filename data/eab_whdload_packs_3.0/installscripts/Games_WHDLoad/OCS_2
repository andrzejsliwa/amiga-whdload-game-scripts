; Copy 'AtomSmasher' entry directory
IF EXISTS "USERPACKAGEDIR:A/AtomSmasher.info"
  Copy >NIL: "USERPACKAGEDIR:A/AtomSmasher.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "A/AtomSmasher"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:A/AtomSmasher"
  Copy >NIL: "USERPACKAGEDIR:A/AtomSmasher" "$entrydir" ALL
ENDIF

