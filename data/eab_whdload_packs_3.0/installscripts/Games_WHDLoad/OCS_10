ENDIF
IF EXISTS "USERPACKAGEDIR:M/ManhattanDealersFr"
  Copy >NIL: "USERPACKAGEDIR:M/ManhattanDealersFr" "$entrydir" ALL
ENDIF

; Copy 'Manhunter' entry directory
IF EXISTS "USERPACKAGEDIR:M/Manhunter.info"
  Copy >NIL: "USERPACKAGEDIR:M/Manhunter.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "M/Manhunter"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:M/Manhunter"
  Copy >NIL: "USERPACKAGEDIR:M/Manhunter" "$entrydir" ALL
ENDIF

