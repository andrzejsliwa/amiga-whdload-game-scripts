  Copy >NIL: "USERPACKAGEDIR:P/Pinkie.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "P/Pinkie"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:P/Pinkie"
  Copy >NIL: "USERPACKAGEDIR:P/Pinkie" "$entrydir" ALL
ENDIF

