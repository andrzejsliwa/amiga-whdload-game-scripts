; Copy 'NoSecondPrize' entry directory
IF EXISTS "USERPACKAGEDIR:N/NoSecondPrize.info"
  Copy >NIL: "USERPACKAGEDIR:N/NoSecondPrize.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "N/NoSecondPrize"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:N/NoSecondPrize"
  Copy >NIL: "USERPACKAGEDIR:N/NoSecondPrize" "$entrydir" ALL
ENDIF

