  Copy >NIL: "USERPACKAGEDIR:P/PinballMagic.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "P/PinballMagic"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:P/PinballMagic"
  Copy >NIL: "USERPACKAGEDIR:P/PinballMagic" "$entrydir" ALL
ENDIF

