; Copy 'MysteriousWorlds' entry directory
IF EXISTS "USERPACKAGEDIR:M/MysteriousWorlds.info"
  Copy >NIL: "USERPACKAGEDIR:M/MysteriousWorlds.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "M/MysteriousWorlds"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:M/MysteriousWorlds"
  Copy >NIL: "USERPACKAGEDIR:M/MysteriousWorlds" "$entrydir" ALL
ENDIF

