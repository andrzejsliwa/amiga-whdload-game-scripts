; Copy 'WarlockTheAvenger' entry directory
IF EXISTS "USERPACKAGEDIR:W/WarlockTheAvenger.info"
  Copy >NIL: "USERPACKAGEDIR:W/WarlockTheAvenger.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "W/WarlockTheAvenger"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:W/WarlockTheAvenger"
  Copy >NIL: "USERPACKAGEDIR:W/WarlockTheAvenger" "$entrydir" ALL
ENDIF

