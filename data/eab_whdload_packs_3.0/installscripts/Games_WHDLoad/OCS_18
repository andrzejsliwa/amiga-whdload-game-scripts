; Copy 'Whirligig' entry directory
IF EXISTS "USERPACKAGEDIR:W/Whirligig.info"
  Copy >NIL: "USERPACKAGEDIR:W/Whirligig.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "W/Whirligig"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:W/Whirligig"
  Copy >NIL: "USERPACKAGEDIR:W/Whirligig" "$entrydir" ALL
ENDIF

