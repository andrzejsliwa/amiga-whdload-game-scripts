; Copy 'XP8' entry directory
IF EXISTS "USERPACKAGEDIR:X/XP8.info"
  Copy >NIL: "USERPACKAGEDIR:X/XP8.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "X/XP8"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:X/XP8"
  Copy >NIL: "USERPACKAGEDIR:X/XP8" "$entrydir" ALL
ENDIF

