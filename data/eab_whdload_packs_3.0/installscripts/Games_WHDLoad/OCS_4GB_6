; Copy 'Gazza2' entry directory
IF EXISTS "USERPACKAGEDIR:G/Gazza2.info"
  Copy >NIL: "USERPACKAGEDIR:G/Gazza2.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "G/Gazza2"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:G/Gazza2"
  Copy >NIL: "USERPACKAGEDIR:G/Gazza2" "$entrydir" ALL
ENDIF

