set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "I"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:I.info"
  Copy >NIL: "USERPACKAGEDIR:I.info" "$INSTALLDIR"
ENDIF

; Copy 'IanBothamsCricket' entry directory
IF EXISTS "USERPACKAGEDIR:I/IanBothamsCricket.info"
  Copy >NIL: "USERPACKAGEDIR:I/IanBothamsCricket.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "I/IanBothamsCricket"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:I/IanBothamsCricket"
  Copy >NIL: "USERPACKAGEDIR:I/IanBothamsCricket" "$entrydir" ALL
ENDIF

