; Copy 'TotalFootball' entry directory
IF EXISTS "USERPACKAGEDIR:T/TotalFootball.info"
  Copy >NIL: "USERPACKAGEDIR:T/TotalFootball.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "T/TotalFootball"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:T/TotalFootball"
  Copy >NIL: "USERPACKAGEDIR:T/TotalFootball" "$entrydir" ALL
ENDIF

