; Copy 'WorldGames' entry directory
IF EXISTS "USERPACKAGEDIR:W/WorldGames.info"
  Copy >NIL: "USERPACKAGEDIR:W/WorldGames.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "W/WorldGames"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:W/WorldGames"
  Copy >NIL: "USERPACKAGEDIR:W/WorldGames" "$entrydir" ALL
ENDIF

