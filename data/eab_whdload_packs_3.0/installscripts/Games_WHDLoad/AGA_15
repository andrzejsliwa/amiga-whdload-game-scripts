; Copy 'SlightlyMagic' entry directory
IF EXISTS "USERPACKAGEDIR:S/SlightlyMagic.info"
  Copy >NIL: "USERPACKAGEDIR:S/SlightlyMagic.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "S/SlightlyMagic"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:S/SlightlyMagic"
  Copy >NIL: "USERPACKAGEDIR:S/SlightlyMagic" "$entrydir" ALL
ENDIF

