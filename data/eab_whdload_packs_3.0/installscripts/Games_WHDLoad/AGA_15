  Copy >NIL: "USERPACKAGEDIR:S/SpaceQuest3De.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "S/SpaceQuest3De"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:S/SpaceQuest3De"
  Copy >NIL: "USERPACKAGEDIR:S/SpaceQuest3De" "$entrydir" ALL
ENDIF

