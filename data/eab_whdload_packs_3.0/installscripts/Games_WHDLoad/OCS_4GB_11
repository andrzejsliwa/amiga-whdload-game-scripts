  Copy >NIL: "USERPACKAGEDIR:S/SpacePilot89.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "S/SpacePilot89"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:S/SpacePilot89"
  Copy >NIL: "USERPACKAGEDIR:S/SpacePilot89" "$entrydir" ALL
ENDIF

