; Copy 'RVFHonda' entry directory
IF EXISTS "USERPACKAGEDIR:R/RVFHonda.info"
  Copy >NIL: "USERPACKAGEDIR:R/RVFHonda.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "R/RVFHonda"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:R/RVFHonda"
  Copy >NIL: "USERPACKAGEDIR:R/RVFHonda" "$entrydir" ALL
ENDIF

; 'S' index directory
echo "Installing 'S'..."
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "S"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:S.info"
  Copy >NIL: "USERPACKAGEDIR:S.info" "$INSTALLDIR"
ENDIF

