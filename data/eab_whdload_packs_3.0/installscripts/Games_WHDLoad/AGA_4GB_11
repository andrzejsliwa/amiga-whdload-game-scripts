; Copy 'Shinobi' entry directory
IF EXISTS "USERPACKAGEDIR:S/Shinobi.info"
  Copy >NIL: "USERPACKAGEDIR:S/Shinobi.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "S/Shinobi"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:S/Shinobi"
  Copy >NIL: "USERPACKAGEDIR:S/Shinobi" "$entrydir" ALL
ENDIF

