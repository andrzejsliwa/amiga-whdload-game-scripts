  Copy >NIL: "USERPACKAGEDIR:S/SpikeInTransylvania.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "S/SpikeInTransylvania"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:S/SpikeInTransylvania"
  Copy >NIL: "USERPACKAGEDIR:S/SpikeInTransylvania" "$entrydir" ALL
ENDIF

