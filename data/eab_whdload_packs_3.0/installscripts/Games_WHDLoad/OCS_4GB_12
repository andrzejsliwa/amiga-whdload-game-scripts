; Copy 'SpyVsSpy2TheIslandCaper' entry directory
IF EXISTS "USERPACKAGEDIR:S/SpyVsSpy2TheIslandCaper.info"
  Copy >NIL: "USERPACKAGEDIR:S/SpyVsSpy2TheIslandCaper.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "S/SpyVsSpy2TheIslandCaper"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:S/SpyVsSpy2TheIslandCaper"
  Copy >NIL: "USERPACKAGEDIR:S/SpyVsSpy2TheIslandCaper" "$entrydir" ALL
ENDIF

