  Copy >NIL: "USERPACKAGEDIR:T/Tetrix.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "T/Tetrix"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:T/Tetrix"
  Copy >NIL: "USERPACKAGEDIR:T/Tetrix" "$entrydir" ALL
ENDIF

