  Copy >NIL: "USERPACKAGEDIR:T/TarghanFr.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "T/TarghanFr"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:T/TarghanFr"
  Copy >NIL: "USERPACKAGEDIR:T/TarghanFr" "$entrydir" ALL
ENDIF

