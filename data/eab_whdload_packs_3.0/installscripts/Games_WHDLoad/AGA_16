; Copy 'Superman' entry directory
IF EXISTS "USERPACKAGEDIR:S/Superman.info"
  Copy >NIL: "USERPACKAGEDIR:S/Superman.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "S/Superman"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:S/Superman"
  Copy >NIL: "USERPACKAGEDIR:S/Superman" "$entrydir" ALL
ENDIF

