; Copy 'Numeric2' entry directory
IF EXISTS "USERPACKAGEDIR:N/Numeric2.info"
  Copy >NIL: "USERPACKAGEDIR:N/Numeric2.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "N/Numeric2"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:N/Numeric2"
  Copy >NIL: "USERPACKAGEDIR:N/Numeric2" "$entrydir" ALL
ENDIF

; Copy 'Numeric3' entry directory
