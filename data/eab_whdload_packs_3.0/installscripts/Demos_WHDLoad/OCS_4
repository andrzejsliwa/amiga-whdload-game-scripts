  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:S/StatusQuo"
  Copy >NIL: "USERPACKAGEDIR:S/StatusQuo" "$entrydir" ALL
ENDIF

