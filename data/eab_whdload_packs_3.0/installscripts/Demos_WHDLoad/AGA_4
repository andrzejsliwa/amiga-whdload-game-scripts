; Copy 'MusicDiskThrust' entry directory
IF EXISTS "USERPACKAGEDIR:M/MusicDiskThrust.info"
  Copy >NIL: "USERPACKAGEDIR:M/MusicDiskThrust.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "M/MusicDiskThrust"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:M/MusicDiskThrust"
  Copy >NIL: "USERPACKAGEDIR:M/MusicDiskThrust" "$entrydir" ALL
ENDIF

