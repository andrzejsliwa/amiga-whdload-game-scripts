  Copy >NIL: "USERPACKAGEDIR:G/GreatestHits.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "G/GreatestHits"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:G/GreatestHits"
  Copy >NIL: "USERPACKAGEDIR:G/GreatestHits" "$entrydir" ALL
ENDIF

