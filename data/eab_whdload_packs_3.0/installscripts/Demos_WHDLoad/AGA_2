; Copy 'DemonsRage' entry directory
IF EXISTS "USERPACKAGEDIR:D/DemonsRage.info"
  Copy >NIL: "USERPACKAGEDIR:D/DemonsRage.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "D/DemonsRage"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:D/DemonsRage"
  Copy >NIL: "USERPACKAGEDIR:D/DemonsRage" "$entrydir" ALL
ENDIF

