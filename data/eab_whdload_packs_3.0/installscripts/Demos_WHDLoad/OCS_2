; Copy 'DigitalInnovation' entry directory
IF EXISTS "USERPACKAGEDIR:D/DigitalInnovation.info"
  Copy >NIL: "USERPACKAGEDIR:D/DigitalInnovation.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "D/DigitalInnovation"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:D/DigitalInnovation"
  Copy >NIL: "USERPACKAGEDIR:D/DigitalInnovation" "$entrydir" ALL
ENDIF

