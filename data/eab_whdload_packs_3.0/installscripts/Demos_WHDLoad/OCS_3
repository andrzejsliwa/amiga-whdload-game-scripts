; Copy 'MegaDemo2NorthStar' entry directory
IF EXISTS "USERPACKAGEDIR:M/MegaDemo2NorthStar.info"
  Copy >NIL: "USERPACKAGEDIR:M/MegaDemo2NorthStar.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "M/MegaDemo2NorthStar"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:M/MegaDemo2NorthStar"
  Copy >NIL: "USERPACKAGEDIR:M/MegaDemo2NorthStar" "$entrydir" ALL
ENDIF

