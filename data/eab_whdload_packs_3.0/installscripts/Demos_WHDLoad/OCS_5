; Copy 'Technomania' entry directory
IF EXISTS "USERPACKAGEDIR:T/Technomania.info"
  Copy >NIL: "USERPACKAGEDIR:T/Technomania.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "T/Technomania"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:T/Technomania"
  Copy >NIL: "USERPACKAGEDIR:T/Technomania" "$entrydir" ALL
ENDIF

