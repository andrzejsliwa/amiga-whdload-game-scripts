  Copy >NIL: "USERPACKAGEDIR:W/WorldsWave2.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "W/WorldsWave2"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:W/WorldsWave2"
  Copy >NIL: "USERPACKAGEDIR:W/WorldsWave2" "$entrydir" ALL
ENDIF

