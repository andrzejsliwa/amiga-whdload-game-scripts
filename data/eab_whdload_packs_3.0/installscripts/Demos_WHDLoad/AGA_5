; Copy 'SoundDemoSyndicate' entry directory
IF EXISTS "USERPACKAGEDIR:S/SoundDemoSyndicate.info"
  Copy >NIL: "USERPACKAGEDIR:S/SoundDemoSyndicate.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "S/SoundDemoSyndicate"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:S/SoundDemoSyndicate"
  Copy >NIL: "USERPACKAGEDIR:S/SoundDemoSyndicate" "$entrydir" ALL
ENDIF

