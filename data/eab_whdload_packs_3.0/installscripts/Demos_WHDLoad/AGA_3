  Copy >NIL: "USERPACKAGEDIR:M/MegademoAlcatraz.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "M/MegademoAlcatraz"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:M/MegademoAlcatraz"
  Copy >NIL: "USERPACKAGEDIR:M/MegademoAlcatraz" "$entrydir" ALL
ENDIF

