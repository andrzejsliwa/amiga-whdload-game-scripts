; Copy 'LegaliseIt' entry directory
IF EXISTS "USERPACKAGEDIR:L/LegaliseIt.info"
  Copy >NIL: "USERPACKAGEDIR:L/LegaliseIt.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "L/LegaliseIt"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:L/LegaliseIt"
  Copy >NIL: "USERPACKAGEDIR:L/LegaliseIt" "$entrydir" ALL
ENDIF

