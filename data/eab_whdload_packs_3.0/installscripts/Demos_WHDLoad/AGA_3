ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "M/MusicDisk1"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:M/MusicDisk1"
  Copy >NIL: "USERPACKAGEDIR:M/MusicDisk1" "$entrydir" ALL
ENDIF

