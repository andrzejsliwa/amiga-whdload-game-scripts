; Copy 'LordsOfTheRealmAGAFast' entry directory
IF EXISTS "USERPACKAGEDIR:L/LordsOfTheRealmAGAFast.info"
  Copy >NIL: "USERPACKAGEDIR:L/LordsOfTheRealmAGAFast.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "L/LordsOfTheRealmAGAFast"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:L/LordsOfTheRealmAGAFast"
  Copy >NIL: "USERPACKAGEDIR:L/LordsOfTheRealmAGAFast" "$entrydir" ALL
ENDIF

