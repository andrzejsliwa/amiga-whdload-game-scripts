  Copy >NIL: "USERPACKAGEDIR:D/DefenderOfTheCrown2CD32.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "D/DefenderOfTheCrown2CD32"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:D/DefenderOfTheCrown2CD32"
  Copy >NIL: "USERPACKAGEDIR:D/DefenderOfTheCrown2CD32" "$entrydir" ALL
ENDIF

