  Copy >NIL: "USERPACKAGEDIR:H/Heimdall2AGA.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "H/Heimdall2AGA"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:H/Heimdall2AGA"
  Copy >NIL: "USERPACKAGEDIR:H/Heimdall2AGA" "$entrydir" ALL
ENDIF

