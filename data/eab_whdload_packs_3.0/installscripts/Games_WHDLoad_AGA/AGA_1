  Copy >NIL: "USERPACKAGEDIR:B/BombPacCD32AGA.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "B/BombPacCD32AGA"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:B/BombPacCD32AGA"
  Copy >NIL: "USERPACKAGEDIR:B/BombPacCD32AGA" "$entrydir" ALL
ENDIF

