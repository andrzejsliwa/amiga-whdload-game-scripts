; 'A' index directory
echo "Installing 'A'..."

; Copy 'AlterEgo' entry directory
IF EXISTS "USERPACKAGEDIR:HD-Games/AlterEgo.info"
  Copy >NIL: "USERPACKAGEDIR:HD-Games/AlterEgo.info" "$INSTALLDIR"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "AlterEgo"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:HD-Games/AlterEgo"
  Copy >NIL: "USERPACKAGEDIR:HD-Games/AlterEgo" "$entrydir" ALL
ENDIF

; Copy 'AnotherWorldTheLostLevel' entry directory
