; Alter Ego
IF NOT EXISTS "HD-Games:AlterEgo/"
  ECHO "ERROR: Path 'HD-Games:AlterEgo/' doesn't exist!"
ENDIF
IF NOT EXISTS "HD-Games:AlterEgo/alterego"
  ECHO "ERROR: Path 'HD-Games:AlterEgo/alterego' doesn't exist!"
ENDIF
IF NOT EXISTS "HD-Games:AlterEgo/igame.iff"
  ECHO "ERROR: Path 'HD-Games:AlterEgo/igame.iff' doesn't exist!"
ENDIF
IF NOT EXISTS "$ags2gamespath/A.ags/Alter Ego.run"
  ECHO "ERROR: Path '$ags2gamespath/A.ags/Alter Ego.run' doesn't exist!"
ENDIF

