; HstWB Menu Item Run script for AGS2
; -----------------------------------
;RunFile=A-Games:P/PinballFntsiesAGANTSC/PinballFntsiesAGANTSC.slave

; Favourites Mode
IF EXISTS ENV:favouritesmode
  execute S:AGS2Favourites "P" "Pinball Fantasies AGA NTSC"
  IF $favouritesmode EQ "add"
    SKIP end
  ENDIF
  IF $favouritesmode EQ "remove"
    SKIP end
  ENDIF
ENDIF

; Start AGS2 Run Pre Script, if it exists
IF EXISTS S:AGS2RunPre
  execute S:AGS2RunPre
ENDIF

; Whdload
cd "A-Games:P/PinballFntsiesAGANTSC/"
IF EXISTS ENV:whdloadargs
  whdload "PinballFntsiesAGANTSC.slave" $whdloadargs
ELSE
  whdload "PinballFntsiesAGANTSC.slave"
ENDIF

; Start AGS2 Run Post Script, if it exists
IF EXISTS S:AGS2RunPost
  execute S:AGS2RunPost
ENDIF

; End
LAB end
