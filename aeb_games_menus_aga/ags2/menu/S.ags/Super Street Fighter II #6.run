; HstWB Menu Item Run script for AGS2
; -----------------------------------
;RunFile=A-Games:S/SuprStrtFtr2TrboCD32/SuprStrtFtr2TrboCD32.slave

; Favourites Mode
IF EXISTS ENV:favouritesmode
  execute S:AGS2Favourites "S" "Super Street Fighter II #6"
  IF $favouritesmode EQ "add"
    SKIP end
  ENDIF
  IF $favouritesmode EQ "remove"
    SKIP end
  ENDIF
ENDIF

; Start AGS2 Run Pre Script, if it exists
IF EXISTS S:AGS2RunPre
  execute S:AGS2RunPre
ENDIF

; Whdload
cd "A-Games:S/SuprStrtFtr2TrboCD32/"
IF EXISTS ENV:whdloadargs
  whdload "SuprStrtFtr2TrboCD32.slave" $whdloadargs
ELSE
  whdload "SuprStrtFtr2TrboCD32.slave"
ENDIF

; Start AGS2 Run Post Script, if it exists
IF EXISTS S:AGS2RunPost
  execute S:AGS2RunPost
ENDIF

; End
LAB end
