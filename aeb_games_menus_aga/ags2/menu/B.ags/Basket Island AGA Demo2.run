; HstWB Menu Item Run script for AGS2
; -----------------------------------
;RunFile=A-Games:B/BasketIslandDemo2AGA/BasketIslandDemo2AGA.Slave

; Favourites Mode
IF EXISTS ENV:favouritesmode
  execute S:AGS2Favourites "B" "Basket Island AGA Demo2"
  IF $favouritesmode EQ "add"
    SKIP end
  ENDIF
  IF $favouritesmode EQ "remove"
    SKIP end
  ENDIF
ENDIF

; Start AGS2 Run Pre Script, if it exists
IF EXISTS S:AGS2RunPre
  execute S:AGS2RunPre
ENDIF

; Whdload
cd "A-Games:B/BasketIslandDemo2AGA/"
IF EXISTS ENV:whdloadargs
  whdload "BasketIslandDemo2AGA.Slave" $whdloadargs
ELSE
  whdload "BasketIslandDemo2AGA.Slave"
ENDIF

; Start AGS2 Run Post Script, if it exists
IF EXISTS S:AGS2RunPost
  execute S:AGS2RunPost
ENDIF

; End
LAB end
