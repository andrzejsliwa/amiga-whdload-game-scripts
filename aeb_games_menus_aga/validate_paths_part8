  ECHO "ERROR: Path 'A-Games:L/LordsOfTime/LordsOfTime.Slave' doesn't exist!"
ENDIF
IF NOT EXISTS "A-Games:L/LordsOfTime/igame.iff"
  ECHO "ERROR: Path 'A-Games:L/LordsOfTime/igame.iff' doesn't exist!"
ENDIF